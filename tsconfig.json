{
  "compilerOptions": {
    "baseUrl": ".",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": ["ES2015"],
    "module": "CommonJS",
    "noFallthroughCasesInSwitch": true,
    "noImplicitReturns": true,
    "outDir": "dist",
    "paths": {
      "@bitgo/utxo-lib": ["src/bitgo__utxo-lib"]
    },
    "sourceMap": true,
    "strict": true,
    "target": "ES2015",
  }
}
